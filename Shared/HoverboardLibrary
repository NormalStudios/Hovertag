local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Hoverboards = ReplicatedStorage:WaitForChild("Hoverboards")

local Defaults = {
	BoostMultiplier = 2,
	BoostCapacity = 300,
	BoostDepleteRate = 1,
	BoostDepleteInterval = .01,
	
	Speed = 100,
	
}

local HoverboardLibrary = {
	["Classic"] = {
		Name = "Classic",
		Description = "Ol' reliable!",
		Rarity = "Common",
		Location = Hoverboards:WaitForChild("Classic"),
		
		BoostMultiplier = Defaults.BoostMultiplier,
		BoostCapacity = Defaults.BoostCapacity,
		BoostDepleteRate = Defaults.BoostDepleteRate,
		BoostDepleteInterval = Defaults.BoostDepleteInterval,

		Speed = Defaults.Speed,
	}	
}

return HoverboardLibrary
